
pub enum TokenTypes {
    Kw,
    Ident,
    Var,
    Str,
    Int (IntDataType),
    UInt(UIntDataType)
    Flt (FltDataType),
}

global enum IntDataType {
    I8,   I16,
    I32,  I64,
    I128, I256,
    I512, ISIZE,
}

global enum UIntDataType {
    U8,   U16,
    U32,  U64,
    U128, U256,
    U512, USIZE,
}

global enum FltDataType {
    F16,  F32,
    F64,  F128,
    F256, F512,
    FSIZE,
}
